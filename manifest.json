{
    "name": "Eye Break", 
    "description": "Extension for eye breaks", 
    "version": "1.0", 
    "manifest_version": 3,
    
    "action": {
        "default_popup": "break.html", 
        "default_icon": "break_extensions.png"
    },

    "permissions": [
        "alarms",
        "notifications"
    ], 


    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            },
            "description": "Opens break.html"
        }
    }
}